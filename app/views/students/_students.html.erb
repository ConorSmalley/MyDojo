<table>
    <thead>
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Dob</th>
        <th></th>
        <%# <th colspan="3"></th> %>
      </tr>
    </thead>

    <tbody>
      <% @students.each do |student| %>
        <tr>
          <td><%= student.first_name %></td>
          <td><%= student.last_name %></td>
          <td><%= student.dob.strftime("%d/%m/%Y") %></td>
          <td>
          <%= link_to 'Show', student %>
        </tr>
      <% end %>
    </tbody>
  </table>